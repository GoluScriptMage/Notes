{
  "name": "typescript-speed-run",
  "version": "1.0.0",
  "description": "Master advanced TypeScript patterns in 3 days + bonus",
  "main": "index.js",
  "scripts": {
    "check": "tsc --noEmit",
    "check:day1": "tsc --noEmit day-1-the-guard/**/*.ts",
    "check:day2": "tsc --noEmit day-2-the-factory/**/*.ts",
    "check:day3": "tsc --noEmit day-3-the-toolkit/**/*.ts",
    "check:bonus": "tsc --noEmit bonus-day-real-world/**/*.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "echo \"No tests yet - this is a learning project!\""
  },
  "keywords": [
    "typescript",
    "learning",
    "generics",
    "utility-types",
    "discriminated-unions",
    "socket.io"
  ],
  "author": "You!",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0"
  }
}
